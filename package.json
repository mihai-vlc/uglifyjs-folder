{
  "name": "uglifyjs-folder",
  "description": "Run terser on a folder and minify the result in a single file",
  "version": "3.2.0",
  "homepage": "https://github.com/ionutvmi/uglifyjs-folder",
  "author": {
    "name": "Mihai Ionut Vilcu",
    "email": "ionutvmi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ionutvmi/uglifyjs-folder.git"
  },
  "bugs": {
    "url": "https://github.com/ionutvmi/uglifyjs-folder/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ionutvmi/uglifyjs-folder/blob/master/LICENSE-MIT"
    }
  ],
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "uglify",
    "folder",
    "directory",
    "cli"
  ],
  "preferGlobal": true,
  "bin": {
    "uglifyjs-folder": "cli.js"
  },
  "scripts": {
    "test": "ava --no-worker-threads",
    "test:watch": "ava --no-worker-threads --watch",
    "coverage": "nyc --reporter=lcov ava --no-worker-threads"
  },
  "ava": {
    "files": [
      "test/index.js",
      "test/snapshots.js"
    ],
    "ignoredByWatcher": [
      "tmp/**"
    ],
    "snapshotDir": "test/snapshots-data"
  },
  "dependencies": {
    "extend": "3.0.2",
    "globby": "11.1.0",
    "graceful-fs": "4.2.10",
    "meow": "9.0.0",
    "mkdirp": "1.0.4",
    "terser": "5.16.1"
  },
  "devDependencies": {
    "ava": "5.1.0",
    "del": "6.1.1",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "sinon": "15.0.1"
  }
}
